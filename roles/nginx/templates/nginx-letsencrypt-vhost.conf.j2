server {
    listen       80 default_server;
    listen       [::]:80 default_server;
    root         /srv/http;
    server_name
    {% for domain in letsencrypt_catchall_domains %}
        {{ domain }}
    {% endfor %}
    ;

    include snippets/letsencrypt.conf;

    location / {
        return 404;
    }
}
